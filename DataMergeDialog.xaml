<Window x:Class="RemnantWorldChanger.DataMergeDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RemnantWorldChanger"
        mc:Ignorable="d"
        Title="DataMergeDialog" Height="200" Width="300">
    <DockPanel>
        <Grid DockPanel.Dock="Bottom" Height="50">
            <Button Content="Continue" HorizontalAlignment="Right"  Margin="0,0,80,0" Width="60" Height="20"/>
            <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,0" Width="60" Height="20"/>
        </Grid>
        <Grid DockPanel.Dock="Top">
            <RadioButton/>
            <RadioButton/>
        </Grid>
    </DockPanel>
</Window>
